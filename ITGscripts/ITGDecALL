#!/bin/bash
#usage: $ ./ITGDecALL
#
# This script will take all log files (name starting with 'log_')
# and decode them.
#
############################################
#DECODE
for file in $(ls log_*)
do
    outputFileName="decoded_${file}.txt"
    outputOctaveFileName="decoded_octave_${file}.csv"
    sudo ITGDec ${file} -l $outputFileName -o $outputOctaveFileName
done
